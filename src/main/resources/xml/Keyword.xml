<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.accp.dao.KeyWordDao">


   <!--动态验证关键词是否存在-->
    <select id="queryByKeyWord" resultType="Keyword">
      SELECT * FROM keyword WHERE keyword=#{keyword}
    </select>

    <!--添加关键词-->
    <insert id="addKeyWord" >
        INSERT INTO keyword (keywordId,keyword,customerId,userId,typeId,term,createTime,keywordStatus) VALUES (#{keywordId},#{keyword},#{customerId},#{userId},#{typeId},#{term},#{createTime},#{keywordStatus})
    </insert>



</mapper>